<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://JAVA.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>mqtt</display-name>
	<servlet>
		<description>
		</description>
		<display-name>GetAirQuality</display-name>
		<servlet-name>GetAirQuality</servlet-name>
		<servlet-class>com.brilcom.ctr.GetAirQuality</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>SensorRegistry</display-name>
		<servlet-name>SensorRegistry</servlet-name>
		<servlet-class>com.brilcom.ctr.SensorRegistry</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AirQualityListener</display-name>
		<servlet-name>AirQualityListener</servlet-name>
		<servlet-class>com.brilcom.ctr.AirQualityListener</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AirQualitySender</display-name>
		<servlet-name>AirQualitySender</servlet-name>
		<servlet-class>com.brilcom.ctr.AirQualitySender</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SensorRegistry</servlet-name>
		<url-pattern>/SensorRegistry</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AirQualityListener</servlet-name>
		<url-pattern>/AirQualityListener</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AirQualitySender</servlet-name>
		<url-pattern>/AirQualitySender</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<filter>
		<filter-name>MultiPartFilter</filter-name>
		<filter-class>org.jepetto.filter.MultiPartFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>MultiPartFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
