<?xml version="1.0" encoding="UTF-8"?>
<web-app
	id="WebApp_ID"
	xmlns="http://Java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee; http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	version="2.4">
	<display-name>MQTT</display-name>
	<servlet>
		<description>
		</description>
		<display-name>SensorRegistry</display-name>
		<servlet-name>SensorRegistry</servlet-name>
		<servlet-class>com.brilcom.ctr.SensorRegistry</servlet-class>
	</servlet>

	<servlet>
		<description>
		</description>
		<display-name>AirQualityListener</display-name>
		<servlet-name>AirQualityListener</servlet-name>
		<servlet-class>com.brilcom.ctr.AirQualityListener</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AirQualitySender</display-name>
		<servlet-name>AirQualitySender</servlet-name>
		<servlet-class>com.brilcom.ctr.AirQualitySender</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetAirQuality</display-name>
		<servlet-name>GetAirQuality</servlet-name>
		<servlet-class>com.brilcom.ctr.GetAirQuality</servlet-class>
	</servlet>
	<servlet>
        <description>
        </description>
        <display-name>GetAirQuality1M</display-name>
        <servlet-name>GetAirQuality1M</servlet-name>
        <servlet-class>com.brilcom.ctr.GetAirQuality1M</servlet-class>
    </servlet>
	<servlet>
        <description>
        </description>
        <display-name>GetAirQualityChart</display-name>
        <servlet-name>GetAirQualityChart</servlet-name>
        <servlet-class>com.brilcom.ctr.GetAirQualityChart</servlet-class>
    </servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetAirQualityBySec</display-name>
		<servlet-name>GetAirQualityBySec</servlet-name>
		<servlet-class>com.brilcom.ctr.GetAirQualityBySec</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetAirQualityForChart</display-name>
		<servlet-name>GetAirQualityForChart</servlet-name>
		<servlet-class>com.brilcom.ctr.GetAirQualityForChart</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetAirQualityResponse</display-name>
		<servlet-name>GetAirQualityResponse</servlet-name>
		<servlet-class>com.brilcom.ctr.GetAirQualityResponse</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AirQualitySender1M</display-name>
		<servlet-name>AirQualitySender1M</servlet-name>
		<servlet-class>com.brilcom.ctr.AirQualitySender1M</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AirQualityRegister1M</display-name>
		<servlet-name>AirQualityRegister1M</servlet-name>
		<servlet-class>com.brilcom.ctr.AirQualitySender1M</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SensorRegistry</servlet-name>
		<url-pattern>/SensorRegistry</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AirQualityListener</servlet-name>
		<url-pattern>/AirQualityListener</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AirQualitySender</servlet-name>
		<url-pattern>/AirQualitySender</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetAirQuality</servlet-name>
		<url-pattern>/GetAirQuality</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
        <servlet-name>GetAirQuality1M</servlet-name>
        <url-pattern>/GetAirQuality1M</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>GetAirQualityChart</servlet-name>
        <url-pattern>/GetAirQualityChart</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetAirQualityBySec</servlet-name>
		<url-pattern>/GetAirQualityBySec</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetAirQualityForChart</servlet-name>
		<url-pattern>/GetAirQualityForChart</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetAirQualityResponse</servlet-name>
		<url-pattern>/GetAirQualityResponse</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AirQualitySender1M</servlet-name>
		<url-pattern>/AirQualitySender1M</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AirQualityRegister1M</servlet-name>
		<url-pattern>/AirQualityRegister1M</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<filter>
		<filter-name>MultiPartFilter</filter-name>
		<filter-class>org.jepetto.filter.MultiPartFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>MultiPartFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter>
		<filter-name>Set Character Encoding</filter-name>
		<filter-class>org.apache.catalina.filters.SetCharacterEncodingFilter
		</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>Set Character Encoding</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>






</web-app>
